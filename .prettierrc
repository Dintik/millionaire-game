{
  "endOfLine": "auto",
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "quoteProps": "preserve",
  "bracketSameLine": true,
  "plugins": ["prettier-plugin-classnames", "prettier-plugin-merge"]
}
